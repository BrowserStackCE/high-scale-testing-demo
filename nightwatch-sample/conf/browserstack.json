{
  "project": "browserstack-examples",
  "build": "browserstack-examples-nightwatchjs",
  "server": "hub-cloud.browserstack.com",
  "defaultUrl": "https://bstackdemo.com",
  "username": "",
  "access_key": "",
  "local_args": {
    "logFile": "local.log",
    "verbose": 3
  },
  "android": {
    "os_version": "10.0",
    "device": "Samsung Galaxy S20",
    "real_mobile": "true",
    "browserName": "chrome"
  },

  "iphone": {
    "os_version": "14",
    "device": "iPhone 12",
    "real_mobile": "true",
    "browserName": "safari"
  },

  "chrome": {
    "browserName": "chrome",
    "browserVersion": "latest",
    "resolution": "1920x1080",
    "browserstack.video": true,
    "goog:chromeOptions": {
      "prefs": {
        "profile.managed_default_content_settings.geolocation": 1
      }
    }
  },

  "firefox": {
    "browserName": "firefox",
    "browserVersion": "latest",
    "resolution": "1920x1080",
    "browserstack.video": true
  },

  "edge": {
    "browserName": "edge",
    "browserVersion": "latest",
    "resolution": "1920x1080",
    "browserstack.video": true
  },

  "safari": {
    "browserName": "safari",
    "browserVersion": "latest",
    "resolution": "1920x1080",
    "browserstack.video": true
  }
}
